include ../common/mixin.jade

head 
    +conf("考试报名")

body
    #testconfirm
        .weui-btn-area
            .weui-btn.weui-btn_primary
                h2 四六级报名
                .weui-cells
                    .weui-cell.weui-cell_select.weui-cell_select-after
                        .weui-cell__hd
